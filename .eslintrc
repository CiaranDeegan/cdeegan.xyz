{
    "root": true,
    "env": {
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/recommended"
    ],
    "rules": {
		"indent": ["error", "tab"],
		"vue/html-indent": ["error", "tab"],
		"vue/max-attributes-per-line": "off"
    },
    "parserOptions": {
        "parser": "babel-eslint"
    }
}